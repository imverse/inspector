{
  "name": "@imverse/inspector",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@imverse/replication-client-js": "^1.0.4"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "jsdoc": "^3.5.5",
    "rollup": "^0.63.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "elm2esm": "node elm2esm main.js",
    "make": "elm make main.elm --output main.js",
    "watch": "chokidar '**/*.elm' -c 'npm run build' --initial",
    "build": "npm run make && npm run elm2esm &&  npm run rollup",
    "rollup": "rollup -c",
    "uglify": "uglifyjs dist/index.js --output dist/index.js --beautify beautify=false,quote_style=3",
    "build:dev": "npm run build",
    "build:prod": "npm run build && npm run uglify"
  },
